#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules/luamin/bin/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules/luamin/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules/luamin/bin/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules/luamin/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/luamin@1.0.4/node_modules:/home/allen/projects/computercraft/computercraft-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../luamin/bin/luamin" "$@"
else
  exec node  "$basedir/../luamin/bin/luamin" "$@"
fi
